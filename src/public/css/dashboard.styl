
.content-list,
.log-list table
    font-size: 14px

.log-list button,
.content-list button
    margin: 0

#dashboard .blog-list,
#dashboard .content-list,
#dashboard .static-files,
#dashboard .log-list,
#dashboard .backup-area
    margin-bottom: 40px
    clear: both
    border-left: 1px solid #333333
    padding-left: 20px

#dashboard .backup-database
    display: block
    border-bottom: 1px solid #efefef

.blog-row,
.content-row
    padding: 20px
    background-color: #f7f7f7
    border-radius: 4px
    margin-bottom: 6px

.blog-row .title,
.content-row .title
    margin: 0
    font-size: 18px
    margin-bottom: 10px

.blog-row .title a,
.content-row .title a
    color: #000000
    text-decoration: none

.blog-row .date,
.content-row .date,
.blog-row .author
    font-size: 13px

.blog-row .subtitle
    margin: 12px 0

a.edit-link
    color: $orange

.basic-link
    display: inline-block
    margin: 10px
    cursor: pointer

.log-list .basic-Link
    margin-bottom: 20px

ul.files-list
    list-style-type: none
    border: 1px solid #cccccc
    padding: 0

ul.files-list li
    border-bottom: 1px solid #cccccc
    margin-bottom: 4px

ul.files-list li:last-child
    border-bottom: none

ul.files-list li a
    display: inline-block
    margin: 10px
    color: #000
    text-decoration: none
    font-size: 13px
